{
  "source": {
    "git_repo": {
      "url": "https://url/to/repo.git",
      "username": "repo_username",
      "password": "repo_password",
      "dockerfile": "path/to/Dockerfile",
      "workdir": ".",
      "version": "HEAD"
    }
  },
  "target": {
    "images": [
      {
        "image": "my_image_name",
        "tag": "my_image_tag"
      }
    ]
  }
}